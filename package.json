{
  "name": "codesense",
  "displayName": "CodeSense AI",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "pricing:": "Free Trail",
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "generate-explanation",
          "title": "CodeSense AI",
          "icon": "media/comment-discussion.svg"
        }
      ]
    },
    "views": {
      "generate-explanation": [
        {
          "id": "generate-explanation.account",
          "name": "Account"
        },
        {
          "type": "webview",
          "id": "generate-explanation.explanation",
          "name": "Code Explanation"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "codesense.generatecomment",
          "group": "1_modification"
        },
        {
          "when": "editorHasSelection",
          "command": "codesense.generateexplanation",
          "group": "1_modification"
        }
      ]
    },
    "commands": [
      {
        "command": "codesense.generatecomment",
        "title": "Generate Comment"
      },
      {
        "command": "codesense.generateexplanation",
        "title": "Generate Explanation"
      },
      {
        "command": "codesense.login",
        "title": "Generate Explanation"
      },
      {
        "command": "codesense.logout",
        "title": "Generate Explanation"
      }
    ],
    "keybindings": [
      {
        "command": "codesense.generatecomment",
        "key": "ctrl+shift+/"
      },
      {
        "command": "codesense.generateexplanation",
        "key": "ctrl+shift+f12"
      }
    ],
    "viewsWelcome": [
      {
        "view": "generate-explanation.account",
        "contents": "Enhance your developing experience. Use CodeSense AI to instantly generate comments and explain code. [Learn More](https://codesenseai.com) \n [ Log In ](command:codesense.login)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "axios": "^1.4.0",
    "firebase": "^9.22.1",
    "openai": "^3.2.1",
    "uuid": "^9.0.0"
  }
}
