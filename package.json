{
  "name": "firstextension",
  "displayName": "FirstExtension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:generate-explanation.explanation"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "generate-explanation",
          "title": "Generated Explanation",
          "icon": "media/comment-discussion.svg"
        }
      ]
    },
    "views": {
      "generate-explanation": [
        {
          "type": "webview",
          "id": "generate-explanation.explanation",
          "name": "Explanation"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "firstextension.generatecomment",
          "group": "1_modification"
        },
        {
          "when": "editorHasSelection",
          "command": "firstextension.generateexplanation",
          "group": "1_modification"
        }
      ]
    },
    "commands": [
      {
        "command": "firstextension.generatecomment",
        "title": "Generate Comment"
      },
      {
        "command": "firstextension.generateexplanation",
        "title": "Generate Explanation"
      },
      {
        "command": "firstextension.addexp",
        "title": "Add Explanation"
      }
    ],
    "keybindings": [
      {
        "command": "firstextension.generatecomment",
        "key": "ctrl+shift+/"
      },
      {
        "command": "firstextension.generateexplanation",
        "key": "ctrl+shift+f12"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "openai": "^3.2.1"
  }
}
